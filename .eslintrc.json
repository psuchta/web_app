{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jquery": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "rules": {
      "no-plusplus": "off",
      "no-alert": "off",
      "import/prefer-default-export": "off",
      "import/no-unresolved": "off",
      "class-methods-use-this": "off",
      "comma-dangle": "off"
    }
}
